import java.util.Stack;

public class main {

	public static void main(String[] args) {
	
		       Stack<String> animals= new Stack<>();
		        // add element to stack
		       
		       animals.push("dog");
		       animals.push("hourse");
		       animals.push("cat");
		       
		       System.out.println("stack: " + animals);
		       

	}

}
